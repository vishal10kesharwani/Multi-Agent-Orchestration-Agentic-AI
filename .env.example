# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/multiagent_db
REDIS_URL=redis://localhost:6379/0

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
SECRET_KEY=your-secret-key-here

# LLM Configuration
OPENAI_API_BASE=BASE_URL
OPENAI_API_KEY="API_KEY"
LLM_MODEL=azure_ai/genailab-maas-DeepSeek-V3-0324

# Agent Configuration
MAX_CONCURRENT_AGENTS=10
AGENT_TIMEOUT=300
TASK_RETRY_LIMIT=3

# Monitoring
LOG_LEVEL=INFO
ENABLE_METRICS=true
METRICS_PORT=9090
